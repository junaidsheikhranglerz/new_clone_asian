@model List<ranglerz_project.ModelsBO.EmployeeAttendenceReports>
@{
    ViewBag.Title = "AttendenceReports";
}









    <div class="container body">


        <div class="main_container">

            @{
                Html.RenderPartial("_Partial");
            }

            <!-- top navigation -->
            <!-- /top navigation -->
            <!-- page content -->
            <div class="right_col col-md-10 pull-right" role="main">
                <div class="">
                    <div class="page-title">
                        <div class="title_left">
                            <h3>
                              +Attendence Report
                            </h3>
                        </div>


                    </div>
                    <div class="clearfix"></div>
                    <div class="row">
                        

                    </div>

                    <div class="row">

                        <div class="col-md-12 col-sm-12 col-xs-12">
                            <div class="x_panel">
                                <div class="x_title">
                                    <h2>View Attendence Report</h2>
                                    <div class="clearfix"></div>
                                </div>
                                <div class="x_content">
                                    <table id="example" class="table table-striped responsive-utilities jambo_table">
                                        <thead>
                                            <tr class="headings">
                                                <th>ID</th>
                                                <th>Name </th>
                                                <th>Total Presents </th>
                                                <th>Total Absents </th>                                               

                                            </tr>
                                        </thead>

                                        <tbody>

                                            @foreach (var v in Model)
                                            {
                                             
                                                
                                                    <tr class="even pointer">
                                                        <td>@v.employee.Id</td>
                                                        <td>@v.Name</td>
                                                        <td>@v.presents</td>
                                                        <td>@v.absents</td>



                                                    </tr>
                                                }

                                            
                                        </tbody>

                                    </table>
                                </div>
                            </div>
                        </div>

                        <br />
                        <br />
                        <br />

                    </div>
                </div>

            </div>
            <!-- /page content -->
        </div>

    </div>


    <script type="text/javascript">
    function confirm_delete() {
        return confirm('are you sure?');
    }
    </script>














